
@{
    ViewBag.Title = "DisplayReg";
}

<div class="loader"></div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
<script type="text/javascript">
    $(".btn").click(function () {
        $(".loader").fadeOut("slow");
    });
</script>
<div ng-app="RegAdminReq" ng-controller="ReqAdminController">
    <div class="container ">
        <div class="col-md-12">
            <div class="card card-primary border-primary">
                <div class="card-header bg-primary text-white"> Request For New Created Co-Operative Society </div>
                <div class="card-body">
                    <div class="col-md-4 float-md-right" style="margin-bottom:10px;"><input type="text" placeholder="Search..." class="form-control" ng-model="test"></div>

                    <table class="table table-striped table-responsive-md">
                        <thead class="bg-warning">
                            <tr>
                                <th>Id</th>
                                <th>Name</th>
                              
                                <th>Group Name</th>
                                <th>Address</th>
                                <th>E-mail</th>

                                <th>Mobile Number</th>
                                <th></th>
                                <th></th>
                            </tr>

                        </thead>

                        <tbody>
                            <tr ng-repeat="k in Reg | filter : test">
                                <td>{{ $index + 1 }}</td>

                                <td>{{k.name}}</td>
                                <td>{{k.gname}}</td>
                                <td>{{k.addr}}</td>
                                <td>{{k.email}}</td>
                                <td>{{k.monum}}</td>
                                <td>

                                    <button type="submit" ng-click="AppNewGroupComp(k)" class="btn btn-primary">
                                        Accept
                                        @*<span ng-show="spin.kk==true">
                                                <i class="fa fa-check fa-spinner fa-spin"></i>
                                            </span>Accept*@
                                    </button>
                                </td>
                                <td>
                                    <button type="button" ng-click="RejectCompanyNewGroup(k)" class="btn btn-danger">Reject</button>

                                </td>
                            </tr>
                        <tbody>
                    </table>

                </div>
            </div>
        </div>


        <div class="col-md-12  " style="margin-top:10px;">
            <div class="card card-primary border-primary">
                <div class="card-header bg-primary text-white"> Request For New Company </div>
                <div class="card-body">
                    <div class="col-md-4 float-md-right" style="margin-bottom:10px;"><input type="text" placeholder="Search..." class="form-control" ng-model="Registration"></div>

                    <table class="table table-striped table-responsive-md" style="width:100%">
                        <thead class="bg-warning">
                            <tr>
                                <th>Id</th>
                                <th>Name</th>

                                <th>Group Name</th>
                                <th>Address</th>
                                <th>E-mail</th>

                                <th>Mobile Number</th>
                                <th></th>
                                <th></th>


                            </tr>

                        </thead>

                        <tbody>
                            <tr ng-repeat="c in Regn | filter : Registration">
                                <td>{{ $index + 1 }}</td>
                                <td>{{c.name}}</td>
                                <td>{{c.gname}}</td>
                                <td>{{c.addr}}</td>
                                <td>{{c.email}}</td>
                                <td>{{c.monum}}</td>
                                <td>

                                    <button type="submit" ng-click="AppNewGroupComp(c)" class="btn btn-primary">
                                        Accept
                                        @*<span ng-show="spin.kk==true">
                                                <i class="fa fa-check fa-spinner fa-spin"></i>
                                            </span>Accept*@
                                    </button>
                                </td>
                                <td>


                                    <button type="button" ng-click="RejectCompanyNewGroup(c)" class="btn btn-danger">Reject</button>





                                </td>
                            </tr>
                        <tbody>
                    </table>

                </div>
            </div>
        </div>


    </div>
</div>
